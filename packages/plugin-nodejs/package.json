{
  "name": "@komondor-lab/plugin-nodejs",
  "version": "0.0.0",
  "description": "komondor plugin for nodejs",
  "keywords": [
    "komondor-plugin"
  ],
  "homepage": "https://github.com/unional/komondor",
  "bugs": {
    "url": "https://github.com/unional/komondor/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/komondor.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "files": [
    "lib",
    "libm"
  ],
  "main": "lib/index.js",
  "module": "libm/index.js",
  "jsnext:main": "libm/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.es5.json && tsc -p tsconfig.esnext.json",
    "clean": "rimraf lib libm .ts",
    "dc": "yarn dependency-check",
    "dependency-check": "dependency-check . --unused --no-dev -i '@komondor-lab/core' && dependency-check . --missing --no-dev",
    "verify": "rimraf lib && tsc -p tsconfig.build.json && jest --coverage && yarn dependency-check"
  },
  "dependencies": {
    "@komondor-lab/core": "^7.0.0-alpha"
  },
  "engines": {
    "node": ">= 6"
  }
}
