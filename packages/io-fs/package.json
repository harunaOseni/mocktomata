{
  "name": "@moctomata/io-fs",
  "version": "7.0.0-alpha",
  "description": "moctomata file system IO package",
  "homepage": "https://github.com/moctomata/moctomata",
  "bugs": {
    "url": "https://github.com/moctomata/moctomata/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moctomata/moctomata.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "files": [
    "lib",
    "libm"
  ],
  "main": "lib/index.js",
  "module": "libm/index.js",
  "jsnext:main": "libm/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "yarn build:es5 && yarn build:esnext",
    "build:es5": "tsc -p tsconfig.es5.json",
    "build:esnext": "tsc -p tsconfig.esnext.json",
    "clean": "rimraf lib libm .ts",
    "coverage": "jest --coverage --reporters=default",
    "dc": "yarn dependency-check",
    "dependency-check": "dependency-check . --unused --no-dev -i @moctomata/framework && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --reporters=default",
    "test:watch": "jest --watch",
    "verify": "yarn lint && yarn build && yarn dc && yarn coverage",
    "watch": "yarn test:watch"
  },
  "dependencies": {
    "@moctomata/framework": "^7.0.0-alpha",
    "find-installed-packages": "^2.0.1",
    "global-store": "^1.0.0-beta.11",
    "make-error": "^1.3.5",
    "md5": "^2.2.1",
    "mkdirp": "^0.5.1",
    "unpartial": "^0.6.3"
  },
  "devDependencies": {
    "@types/md5": "^2.1.33",
    "@types/mkdirp": "^0.5.2",
    "@types/tmp": "^0.1.0",
    "assertron": "^7.1.2",
    "memfs": "^2.15.5",
    "tmp": "^0.1.0"
  },
  "engines": {
    "node": ">= 6"
  }
}
