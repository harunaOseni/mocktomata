  "files": [
    "lib",
    "libm"
  ],
  "main": "lib/index.js",
  "module": "libm/index.js",
  "jsnext:main": "libm/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib libm *.tsbuildinfo",
    "compile": "tsc -p tsconfig.es5.json && tsc -p tsconfig.esnext.json",
    "dc": "yarn dependency-check",
    "dependency-check": "dependency-check . --unused --no-dev -i @komondor-lab/plugin && dependency-check . --missing --no-dev",
    "verify": "yarn compile && yarn dc && jest --coverage --reporters=default",
    "watch": "jest --watch"
  },
  "dependencies": {
    "@komondor-lab/plugin": "^7.0.0-alpha",
    "find-installed-packages": "^2.0.0",
    "global-store": "^0.8.1",
    "make-error": "^1.3.5",
    "md5": "^2.2.1",
    "mkdirp": "^0.5.1",
    "unpartial": "^0.6.3"
  },
  "devDependencies": {
    "@types/md5": "^2.1.33",
    "@types/mkdirp": "^0.5.2",
    "@types/tmp": "^0.1.0",
    "assertron": "^7.0.0",
    "memfs": "^2.15.2",
    "tmp": "^0.1.0"
  },
  "engines": {
    "node": ">= 6"
  }
}
