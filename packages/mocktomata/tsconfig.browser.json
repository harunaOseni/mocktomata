{
  "compilerOptions": {
    "allowJs": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "lib": [
      "es2015"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "newLine": "LF",
    "noUnusedLocals": true,
    "removeComments": false,
    "sourceMap": true,
    "strict": true,
    "target": "esnext",
    // "skipLibCheck": true,
    // "outDir": ".no-where",
    "rootDir": "src"
  }
}
