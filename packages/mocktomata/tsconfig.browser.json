{
  "compilerOptions": {
    "declaration": false,
    "esModuleInterop": true,
    "lib": [
      "es2015"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "newLine": "LF",
    "noUnusedLocals": true,
    "removeComments": false,
    "sourceMap": true,
    "strict": true,
    "target": "es2015",
    "skipLibCheck": true,
    "outDir": "b",
    "rootDir": "src"
  }
}
