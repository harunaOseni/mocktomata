{
  "name": "mocktomata",
  "version": "7.0.0-alpha",
  "description": "A Behavior Tracking and Simulation System",
  "keywords": [
    "boundary",
    "boundary-testing",
    "fixture",
    "testing",
    "testing-framework",
    "testing-tools"
  ],
  "homepage": "https://github.com/mocktomata/mocktomata",
  "bugs": {
    "url": "https://github.com/mocktomata/mocktomata/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mocktomata/mocktomata.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "files": [
    "lib",
    "libm",
    "dist"
  ],
  "browser": {},
  "main": "lib/index.js",
  "module": "libm/index.js",
  "jsnext:main": "libm/index.js",
  "typings": "lib/index.d.ts",
  "bin": {
    "komondor": "lib/bin.js"
  },
  "scripts": {
    "build": "yarn build:es5 && yarn build:esnext && yarn build:browser",
    "build:es5": "tsc -p tsconfig.es5.json",
    "build:esnext": "tsc -p tsconfig.esnext.json",
    "build:browser": "webpack",
    "clean": "rimraf lib libm dist .ts",
    "coverage": "jest --coverage --reporters=default",
    "dc": "yarn depcheck",
    "depcheck": "dependency-check . --unused --no-dev -i standard-log-color -i @mocktomata/io-client  && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --reporters=default",
    "test:browser": "karma start",
    "test:watch": "jest --watch",
    "verify": "yarn lint && yarn build && yarn dc && yarn coverage",
    "watch": "yarn test:watch"
  },
  "dependencies": {
    "@mocktomata/file-server": "^7.0.0-beta",
    "@mocktomata/framework": "^7.0.0-beta",
    "@mocktomata/io-client": "^7.0.0-beta",
    "@mocktomata/io-local": "^7.0.0-beta",
    "async-fp": "^2.0.0",
    "chalk": "^3.0.0",
    "clibuilder": "^6.1.7",
    "global-store": "^1.0.0-beta.14",
    "stack-utils": "^2.0.1",
    "standard-log": "^4.0.3",
    "standard-log-color": "^1.5.16",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@mocktomata/plugin-fixture-deep-link": "^1.0.2",
    "@mocktomata/plugin-fixture-dummy": "^1.0.3",
    "@types/delay": "^3.1.0",
    "@types/tmp": "^0.1.0",
    "@unional/devpkg-node": "^1.4.2",
    "babel-loader": "^8.0.6",
    "core-js": "^3.6.1",
    "delay": "^4.3.0",
    "jest": "^24.9.0",
    "karma": "^4.4.1",
    "karma-babel-preprocessor": "^8.0.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^3.1.0",
    "karma-typescript": "^4.1.1",
    "karma-typescript-es6-transform": "^4.1.1",
    "karma-webpack": "^4.0.2",
    "param-case": "^3.0.3",
    "pascal-case": "^3.1.1",
    "source-map-loader": "^0.2.4",
    "tmp": "^0.1.0",
    "ts-loader": "^6.2.1",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10"
  }
}
